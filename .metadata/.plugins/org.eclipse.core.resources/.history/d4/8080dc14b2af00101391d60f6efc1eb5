package ex01;

public class 비교연산자 {

	public static void main(String[] args) {
		
		int numA = 5;
		int numB = 10;
		
		System.out.println(numA == numB);
		System.out.println(numA != numB);
		System.out.println(numA > numB);
		System.out.println(numA < numB);
		System.out.println(numA <= numB);

		
		String s1 = "korea";
		String s2 = "korea";
		
		System.out.println(s1 == s2);
		
		
	}

}
